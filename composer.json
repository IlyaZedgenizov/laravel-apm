{
  "name": "napopravku/laravel-apm",
  "description": "Application monitoring for Laravel",
  "type": "library",
  "authors": [
    {
      "name": "Dmitriy Bykov",
      "email": "d.bykov@napopravku.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "Napopravku\\LaravelAPM\\": "src/"
    }
  },
  "require": {
    "php": "^7.4",
    "laravel/framework": "^7|^8"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Napopravku\\LaravelAPM\\ApmServiceProvider"
      ]
    }
  }
}
