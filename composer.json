{
    "name": "napopravku/laravel-apm",
    "description": "Application monitoring for Laravel",
    "type": "library",
    "authors": [
        {
            "name": "Dmitriy Bykov",
            "email": "d.bykov@napopravku.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Napopravku\\LaravelAPM\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": "^7.4",
        "illuminate/support": "^7.0 | ^8.0",
        "illuminate/contracts": "^7.0 | ^8.0",
        "illuminate/console": "^7.0 | ^8.0",
        "illuminate/queue": "^7.0 | ^8.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "laravel/framework": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Napopravku\\LaravelAPM\\ApmServiceProvider"
            ]
        }
    }
}
